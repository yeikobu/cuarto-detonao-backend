{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ], 
    "functions": {
        "api/**/*.js": {
        "maxDuration": 30
        }
    }
}